function [ total ] = real_classify( image,x,y,alphas)
%Classifies a image window based on adaboost training
window_size = 20;
x1 = x-window_size;
x2 = x+window_size;
y1 = y-window_size;
y2 = y+window_size;
window = image(fixo(x1,image,1):fixo(x2,image,1),fixo(y1,image,0):fixo(y2,image,0));
point= {1,(compare_entropy(window)),(compare_gauss(window,3)),(compare_glcm(window)),0,0,0};

[r,c] = size(alphas);
total = 0;
for col=1:c
    total = total + classify(point,col);
end
total



end

